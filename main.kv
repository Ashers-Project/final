#This is the Page that opens up to control the User Name and Contact Information As well as the App installation Date, Notification Preferences,
<AccountInfoWindow>:
    name: "Account Info Page"
    MDGridLayout:
        canvas.before:
            Color:
                rgba: app.background_primary_color
            Rectangle:
                pos: self.pos
                size: self.size

    MDGridLayout:
        cols:1

        MDTopAppBar:
            title: "Account Settings"
            left_action_items: [["content-save-move", lambda x: app.save_switch_to_main_account()]]
            right_action_items :[['', lambda x : app.secret_menu()]]
            md_bg_color: app.secondary_color


    MDCard:
        size_hint: 1, 0.9
        pos_hint: {'center_x': 0.5, 'center_y': 0.45}
        md_bg_color: app.background_primary_color
        ScrollView:
            do_scroll_y: True
            do_scroll_x: False


            MDGridLayout:
                cols: 1
                size_hint_y: None
                height: self.minimum_height



                MDGridLayout:
                    cols: 1
                    size_hint_y: None
                    height:self.minimum_height
                    padding: dp(10), dp(15),dp(10), dp(0)
                    spacing:dp(15)
                    MDLabel:
                        text: '    User Details'
                        theme_text_color: 'Custom'
                        text_color: 'white'
                        font_size: sp(12)
                        pos_hint_y:{'center_y':0.9}
                    MDCard:
                        size_hint_x: 0.8
                        size_hint_y:None
                        height: dp(50)
                        md_bg_color: app.secondary_color
                        MDGridLayout:
                            cols: 2
                            MDLabel:
                                text: '    First Name'
                                theme_text_color: "Custom"
                                text_color: "white"
                                font_size: sp(16)

                            MDTextField:
                                id: name_input
                                mode: "fill"
                                fill_color_normal: app.secondary_color
                                fill_color_focus: app.secondary_color
                                text_color_normal: "white"
                                multiline : False

                    MDCard:
                        size_hint_x: 0.8
                        size_hint_y:None
                        height: dp(50)
                        md_bg_color: app.secondary_color
                        MDGridLayout:
                            cols: 2
                            MDLabel:
                                text: '    App Installation Date'
                                theme_text_color: "Custom"
                                text_color: "white"
                                font_size: sp(16)

                            MDLabel:
                                id: app_installation_date
                                text: 'April 23, 2024    '
                                theme_text_color: "Custom"
